#!/bin/bash
# Git clone to ~/my/dotfiles and run this.
echo setting up your dotfiles...
ln -s ~/my/dotfiles/emacs/dotemacs ~/.emacs
ln -s ~/my/dotfiles/emacs/emacs.d ~/.emacs.d
if [ -e ~/.bashrc ]; then
    mv ~/.bashrc ~/.bashrc.old
fi
ln -s ~/my/dotfiles/bash/bashrc ~/.bashrc
ln -s ~/my/dotfiles/tmux/tmux.conf ~/.tmux.conf
